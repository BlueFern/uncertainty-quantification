	Journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {ca 2 ϩ,calmodulin-dependent,camp response element-binding protein,cortical astrocytes,mitogen-activated protein kinase,protein kinase c,protein kinase ii,v 1a vasopressin receptor,vasopressin},
	Number = {10},
