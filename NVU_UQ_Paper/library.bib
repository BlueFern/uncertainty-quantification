	Journal = {Physics in Medicine and Biology},
	Keywords = {cerebral metabolic rate of oxygen CMRO2 near infra},
	Number = {15},
	Pages = {2405--2418},
	Pmid = {12953906},
	Title = {{Can the cerebral metabolic rate of oxygen be estimated with near-infrared spectroscopy?}},
