	Issn = {0363-6135},
	Journal = {Am J Physiol Heart Circ Physiol},
	Keywords = {Animals,Calcium-Activated,Calcium-Activated: metabolism,Cardiovascular,Computer Simulation,Cyclic GMP,Cyclic GMP: metabolism,Electric Conductivity,Feedback,Humans,Membrane Potentials,Models,Muscle,Muscle Relaxation,Muscle Relaxation: physiology,Myocytes,Nitric Oxide,Nitric Oxide: metabolism,Physiological,Potassium Channels,Potassium Channels: metabolism,Potassium Channels: physiology,Smooth,Smooth Muscle,Smooth Muscle: metabolism,Smooth Muscle: physiology,Vascular,Vascular: metabolism,Vascular: physiology},
	Number = {2},
