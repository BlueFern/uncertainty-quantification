	Keywords = {Acetylcholine,Acetylcholine: pharmacology,Acetylcholine: physiology,Animals,Arteries,Arteries: drug effects,Arteries: physiology,Biological Clocks,Biological Clocks: physiology,Cricetinae,Electric Impedance,Electric Stimulation,Electrophysiology,Endothelium,Iontophoresis,Male,Membrane Potentials,Membrane Potentials: drug effects,Membrane Potentials: physiology,Mesocricetus,Muscle,Smooth,Vascular,Vascular: drug effects,Vascular: physiology,Vasomotor System,Vasomotor System: drug effects,Vasomotor System: physiology},
	Month = {jul},
	Number = {1},
