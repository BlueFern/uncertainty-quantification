	Journal = {Proceedings of the National Academy of Sciences},
	Number = {4},
	Pages = {1461--1465},
	Pmid = {2304911},
	Publisher = {National Acad Sciences},
	Title = {{Minimal model for signal-induced Ca2+ oscillations and for their frequency encoding through protein phosphorylation.}},
	Volume = {87},
