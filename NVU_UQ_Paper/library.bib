	Keywords = {Animals,Calcium Channels,Calcium Channels: physiology,Feedback, Physiological,Female,Hydrogen-Ion Concentration,Male,Membrane Potentials,Photoreceptor Cells,Photoreceptor Cells: physiology,Rats,Rats, Sprague-Dawley,Receptors, GABA,Receptors, GABA: physiology,Retinal Horizontal Cells,Retinal Horizontal Cells: physiology,gamma-Aminobutyric Acid,gamma-Aminobutyric Acid: physiology},
	Pages = {3309--24},
