	Journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {a 1 receptors and,adenosine,de,glutamate,hippocampus,hypoxia,its neuroprotective role,might be involved in,nmda receptors,synaptic transmission},
