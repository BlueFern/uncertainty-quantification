	Keywords = {astrocytes,con-,hippocampus,in-,metabotropic glutamate receptor,mitter release,neurotrans-,synaptic plasticity,tetanus toxin,tracellular calcium oscillations,visual cortex},
	Number = {20},
	Pages = {7817--7830},
	Pmid = {9315902},
	Title = {{Intracellular calcium oscillations in astrocytes: a highly plastic, bidirectional form of communication between neurons and astrocytes in situ.}},
