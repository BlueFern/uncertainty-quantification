	Isbn = {1469-7793 (Electronic)$\backslash$r0022-3751 (Linking)},
	Issn = {1469-7793},
	Journal = {The Journal of physiology},
	Keywords = {Animals,Astrocytes,Astrocytes: physiology,CA1 Region,Chlorides,Chlorides: physiology,GABA-A,GABA-A: physiology,Gap Junctions,Gap Junctions: physiology,Hippocampal,Hippocampal: cytology,Hippocampal: physiology,Inbred C57BL,Inhibitory Postsynaptic Potentials,Male,Mice,Receptors,Synapses,Synapses: physiology,Transgenic,gamma-Aminobutyric Acid,gamma-Aminobutyric Acid: physiology},
