	Keywords = {cerebral metabolic rate of,neurovascular coupling,oxygen,somatosensory cortex},
	Number = {2},
	Pages = {311--322},
	Pmid = {19794398},
	Title = {{Pharmacological uncoupling of activation induced increases in CBF and CMRO2.}},
