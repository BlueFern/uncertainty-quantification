	Keywords = {Calcium,Desensitization,G protein,IP3,PIP2,PLC,Receptors,Sequestration},
	Number = {1},
	Pages = {93--111},
	Pmid = {12782119},
	Title = {{Metabotropic receptor activation, desensitization and sequestration - I: Modelling calcium and inositol 1,4,5-trisphosphate dynamics following receptor activation}},
