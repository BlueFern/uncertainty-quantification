	Issn = {0022-1295},
	Journal = {The Journal of General Physiology},
	Number = {5},
	Pages = {633--647},
	Pmid = {23589582},
	Title = {{Imaging calcium microdomains within entire astrocyte territories and endfeet with GCaMPs expressed using adeno-associated viruses}},
