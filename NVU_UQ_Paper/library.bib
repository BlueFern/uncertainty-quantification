	Issn = {0098-3500},
	Journal = {ACM Trans. Math. Softw.},
	Number = {3},
	Pages = {363--396},
	Title = {{SUNDIALS: Suite of Nonlinear and Differential/Algebraic Equation Solvers}},
	Url = {http://doi.acm.org/10.1145/1089014.1089020{\%}5Cnhttp://dl.acm.org/ft{\_}gateway.cfm?id=1089020{\&}type=pdf},
