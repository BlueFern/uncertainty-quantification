	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {Animals,Cattle,Cells,Connexin 43,Connexin 43: physiology,Cultured,Endothelium,Gap Junctions,Gap Junctions: physiology,Gene Expression Regulation,Gene Expression Regulation: physiology,Hemodynamics,Mechanical,Stress,Vascular,Vascular: pathology,Vascular: physiology,Vascular: ultrastructure},
	Number = {6},
	Pages = {3154--3159},
	Pmid = {10077653},
	Title = {{Spatial and temporal regulation of gap junction connexin43 in vascular endothelial cells exposed to controlled disturbed flows in vitro.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=15911{\&}tool=pmcentrez{\&}rendertype=abstract},
