@book{David2013c,
	Abstract = {The brain needs a constant supply of oxygen and nutrients (essentially glucose). Normal day-to-day life provides a variety of pressure variations in the body and the brain needs to react to these. It does so by varying the resistance of the vasculature, a process known as autoregulation. There are a number of different mechanisms that allow this to occur. Myogenic regulation allows for variations in the arterial radius due to systemic pressure variations. Fluid shear stress exerted on the monocellular layer called the endothelium activates ion channels in both the underlying smooth muscle cells and the endothelium. The ions transported into the cells cause the smooth muscle cells to either contract or dilate. The third mechanism is metabolic in its form. Here the rate of oxygen delivery can allow variations in arteriolar diameters again by cellular variations in calcium ions.We present below numerical and mathematical models which allow investigation of the important pathways, parameters and pathologies of autoregulation. {\textcopyright} 2013 Elsevier Inc. All rights reserved.},
	Author = {David, T. and Brown, R. G.},
	Booktitle = {Transport in Biological Media},
	Doi = {10.1016/B978-0-12-415824-5.00007-2},
	Isbn = {9780124158245},
	Keywords = {Cerebral perfusion,Multiscale model,Neurovascular coupling},
	Pages = {253--273},
	Publisher = {Elsevier Inc.},
