	Issn = {1529-2401},
	Journal = {The Journal of neuroscience : the official journal of the Society for Neuroscience},
	Keywords = {action,bapta,bis-fura-2,carbachol,dendrite,egta,endoplasmic reticulum,furaptra,in addition to these,ip 3 receptor,mechanisms,metabo-,pyramidal neuron,tropic receptor,we recently showed that},
	Number = {22},
	Pages = {8365--8376},
	Pmid = {11069943},
	Title = {{Inositol 1,4,5-trisphosphate (IP3)-mediated Ca2+ release evoked by metabotropic agonists and backpropagating action potentials in hippocampal CA1 pyramidal neurons.}},
