	Journal = {Brain : a journal of neurology},
	Keywords = {Adult,Animals,Chronic Disease,Electric Stimulation,Electric Stimulation: methods,Epilepsy,Female,Fluorescent Dyes,Fluorescent Dyes: diagnostic use,Hippocampus,Hippocampus: metabolism,Hippocampus: pathology,Hippocampus: physiopathology,Humans,Male,Membrane Potentials,Membrane Potentials: physiology,Metabolic Diseases,Metabolic Diseases: metabolism,Metabolic Diseases: pathology,Metabolic Diseases: physiopathology,Middle Aged,Mitochondria,Mitochondria: metabolism,Mitochondria: physiology,Muscarinic Agonists,NADP,NADP: metabolism,Neurons,Neurons: metabolism,Neurons: physiology,Pilocarpine,Potassium,Potassium: metabolism,Rats,Rhodamine 123,Rhodamine 123: diagnostic use,Sclerosis,Sclerosis: metabolism,Sclerosis: pathology,Sclerosis: physiopathology,Temporal Lobe,Temporal Lobe: metabolism,Temporal Lobe: pathology,Temporal Lobe: physiopathology,Wistar},
