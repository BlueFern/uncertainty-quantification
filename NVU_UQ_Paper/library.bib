	Keywords = {Action Potentials,Action Potentials: physiology,Animals,Cerebellum,Cerebellum: metabolism,Energy Metabolism,Energy Metabolism: physiology,Humans,Models,Neocortex,Neocortex: metabolism,Neurological},
	Month = {jul},
	Number = {7},
	Pages = {1222--1232},
	Pmid = {22434069},
	Publisher = {Nature Publishing Group},
	Title = {{Updated energy budgets for neural computation in the neocortex and cerebellum.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=3390818{\&}tool=pmcentrez{\&}rendertype=abstract},
	Volume = {32},
