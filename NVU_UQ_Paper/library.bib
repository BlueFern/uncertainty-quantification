	Keywords = {Animals,Biophysical Phenomena,Biophysics,Brain,Brain: cytology,Brain: metabolism,Calcium,Calcium Channels,Calcium Channels: metabolism,Calcium Signaling,Calcium: metabolism,Extracellular Space,Extracellular Space: metabolism,Ion Transport,Models,Neurological,Neurotransmitter Agents,Neurotransmitter Agents: secretion,Synapses,Synapses: metabolism,Synapses: ultrastructure},
	Number = {4},
