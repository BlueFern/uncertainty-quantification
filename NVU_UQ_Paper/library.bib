	Keywords = {BOLD response,EEG-fMRI,Generalized spike and wave,Idiopathic generalized epilepsy},
	Number = {5},
	Pages = {1127--1144},
	Pmid = {15033899},
	Title = {{fMRI activation during spike and wave discharges in idiopathic generalized epilepsy}},
	Volume = {127},
	Year = {2004},
