	Keywords = {Astrocytic vascular end-feet,Electron microscopy,Excitatory synapses,Lactate,Volume transmitter},
	Number = {10},
	Pages = {2784--2795},
	Pmid = {23696276},
	Title = {{Lactate receptor sites link neurotransmission, neurovascular coupling, and brain energy metabolism}},
