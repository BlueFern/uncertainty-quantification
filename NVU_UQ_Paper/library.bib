	Pmid = {27062231},
	Title = {{3D time-varying simulations of Ca2+ dynamics in Arterial Coupled Cells: a massively parallel implementation}},
	Url = {http://onlinelibrary.wiley.com/doi/10.1002/cnm.2786/suppinfo},
	Volume = {e02786},
