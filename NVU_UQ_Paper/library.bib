	Number = {1 996},
	Pages = {199--210},
	Pmid = {8730595},
	Title = {{Spike after-depolarization and burst generation in adult rat hippocampal CA1 pyramidal cells.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=1158874{\&}tool=pmcentrez{\&}rendertype=abstract},
