	Month = {dec},
	Number = {1},
	Pages = {93--115},
	Pmid = {19292510},
	Title = {{Estimation and identification of parameters in a lumped cerebrovascular model}},
	Url = {http://www.aimsciences.org/journals/displayArticles.jsp?paperID=3894},
	Volume = {6},
	Year = {2008},
	Bdsk-Url-1 = {http://www.aimsciences.org/journals/displayArticles.jsp?paperID=3894},
