@article{Sun2008,
	Abstract = {Reactive nitrogen species (RNS) and oxygen species (ROS) have been reported to modulate the function of nitric oxide synthase (NOS); however, the precise dose-dependent effects of specific RNS and ROS on NOS function are unknown. Questions remain unanswered regarding whether pathophysiological levels of RNS and ROS alter NOS function, and if this alteration is reversible. We measured the effects of peroxynitrite (ONOO-), superoxide (O2{\textperiodcentered} -), hydroxyl radical (.OH), and H2O2 on nNOS activity. The results showed that NO production was inhibited in a dose-dependent manner by all four oxidants, but only O2{\textperiodcentered} - and ONOO- were inhibitory at pathophysiological concentrations (≤50 $\mu$M). Subsequent addition of tetrahydrobiopterin (BH4) fully restored activity after O2{\textperiodcentered} - exposure, while BH4 partially rescued the activity decrease induced by the other three oxidants. Furthermore, treatment with either ONOO- or O2{\textperiodcentered} - stimulated nNOS uncoupling with decreased NO and enhanced O2{\textperiodcentered} - generation. Thus, nNOS is reversibly uncoupled by O2{\textperiodcentered} - (≤50 $\mu$M), but irreversibly uncoupled and inactivated by ONOO-. Additionally, we observed that the mechanism by which oxidative stress alters nNOS activity involves not only BH4 oxidation, but also nNOS monomerization as well as possible degradation of the heme. {\textcopyright} 2008 Elsevier Inc. All rights reserved.},
	Author = {Sun, Jian and Druhan, Lawrence J. and Zweier, Jay L.},
	Doi = {10.1016/j.abb.2008.01.003},
	Isbn = {0003-9861},
	Issn = {00039861},
	Journal = {Archives of Biochemistry and Biophysics},
	Keywords = {Dose-dependent,Electron paramagnetic resonance,Free radical,Hydrogen peroxide,Hydroxyl radical,Monomerization,Neuronal nitric oxide synthase,Peroxynitrite,Superoxide,Tetrahydrobiopterin},
	Number = {2},
	Pages = {126--133},
	Pmid = {18201545},
	Title = {{Dose dependent effects of reactive oxygen and nitrogen species on the function of neuronal nitric oxide synthase}},
	Volume = {471},
	Year = {2008},
