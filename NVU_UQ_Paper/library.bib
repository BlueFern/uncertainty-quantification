	Issn = {0958-0670},
	Journal = {Experimental physiology},
	Keywords = {Animals,Brain,Brain: physiology,Cerebrovascular Circulation,Cerebrovascular Circulation: physiology,Electrophysiology,Models,Neuroglia,Neuroglia: physiology,Neurological,Potassium,Potassium Channels,Potassium Channels: physiology,Potassium: physiology,Signal Transduction},
	Month = {jul},
