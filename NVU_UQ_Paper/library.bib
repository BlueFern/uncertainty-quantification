	Journal = {Journal of cerebral blood flow and metabolism : official journal of the International Society of Cerebral Blood Flow and Metabolism},
	Keywords = {Animals,Arachidonic Acid,Arachidonic Acid: metabolism,Flicker Fusion,Flicker Fusion: physiology,Glucose,Glucose: metabolism,Humans,Hyperemia,Hyperemia: metabolism,Hyperemia: physiopathology,Neurons,Neurons: metabolism,Oxygen,Oxygen: metabolism,Photic Stimulation,Regional Blood Flow,Regional Blood Flow: physiology,Retina,Retina: metabolism,Retina: physiology,Retinal Vessels,Retinal Vessels: physiopathology,Vasodilation,Vasodilation: physiology,arachidonic acid,diabetes,glial cells,nitric oxide,potassium channels},
