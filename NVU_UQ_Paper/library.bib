@article{Boynton1996,
	Abstract = {The linear transform model of functional magnetic resonance imaging (fMRI) hypothesizes that fMRI responses are proportional to local average neural activity averaged over a period of time. This work reports results from three empirical tests that support this hypothesis. First, fMRI responses in human primary visual cortex (V1) depend separably on stimulus timing and stimulus contrast. Second, responses to long-duration stimuli can be predicted from responses to shorter duration stimuli. Third, the noise in the fMRI data is independent of stimulus contrast and temporal period. Although these tests can not prove the correctness of the linear transform model, they might have been used to reject the model. Because the linear transform model is consistent with our data, we proceeded to estimate the temporal fMRI impulse--response function and the underlying (presumably neural) contrast--response function of human V1.},
	Author = {Boynton, Geoffrey M and Engel, Stephen A and Glover, Gary H and Heeger, David J},
	Isbn = {0270-6474 (Print){\$}\backslash{\$}r0270-6474 (Linking)},
	Issn = {0270-6474, 1529-2401},
	Journal = {The Journal of Neuroscience},
	Number = {13},
	Pages = {4207--4221},
	Pmid = {8753882},
	Title = {{Linear Systems Analysis of Functional Magnetic Resonance Imaging in Human V1}},
	Url = {http://www.jneurosci.org/content/16/13/4207{\$}{\%}5C{\$}nhttp://www.jneurosci.org/content/16/13/4207.abstract{\$}{\%}5C{\$}nhttp://www.jneurosci.org/content/16/13/4207.full.pdf},
	Volume = {16},
	Year = {1996}}

@article{Crane1999,
	Abstract = {The spread of membrane potential changes throughout certain cells{\$}\backslash{\$}nand tissues plays an important role in their physiology. The attenuation{\$}\backslash{\$}nof such changes in any tissue is usually characterized by the cable{\$}\backslash{\$}nlength constant lambda, which can be determined experimentally if{\$}\backslash{\$}nthe equations describing membrane potential spread in the tissue{\$}\backslash{\$}nare known. Here we derive an equation describing spread of membrane{\$}\backslash{\$}npotential changes in a short cable, which is an appropriate model{\$}\backslash{\$}nfor short segments of blood vessels. This equation is more general{\$}\backslash{\$}nthan those already published in that the positions of both the current{\$}\backslash{\$}nsource that gives rise to a potential change, and the point at which{\$}\backslash{\$}nthe change is measured, can be anywhere along the cable.},
