	Issn = {00225193},
	Journal = {Journal of Theoretical Biology},
	Keywords = {4,5-trisphosphate,Astrocyte-neuron communication,Epilepsy,Glia,Inositol 1,Spontaneous activity},
	Number = {4},
	Pages = {553--560},
