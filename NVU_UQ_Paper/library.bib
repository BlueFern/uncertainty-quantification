	Keywords = {Animals,Calcium,Calcium: analysis,Calcium: metabolism,Cells,Cultured,Fluorescent Dyes,Fura-2,Microscopy,Muscle,Rabbits,Smooth,Smooth: metabolism,Video},
	Month = {sep},
