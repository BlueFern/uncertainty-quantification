	Journal = {NeuroImage},
	Number = {4},
	Pages = {1915--1922},
	Pmid = {14683697},
	Title = {{fMRI "deactivation" of the posterior cingulate during generalized spike and wave}},
	Volume = {20},
	Year = {2003},
	Bdsk-Url-1 = {https://doi.org/10.1016/S1053-8119(03)00294-5}}

