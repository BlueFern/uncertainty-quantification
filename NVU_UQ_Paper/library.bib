	Keywords = {Animals,Coronary Circulation,Coronary Disease,Coronary Disease: pathology,Coronary Vessels,Coronary Vessels: anatomy {\&} histology,Coronary Vessels: metabolism,Coronary Vessels: physiology,Energy Metabolism,Hemodynamics,Models,Swine,Swine: anatomy {\&} histology,Theoretical},
	Number = {66},
