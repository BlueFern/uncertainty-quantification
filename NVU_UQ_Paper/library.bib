	Doi = {10.1016/j.freeradbiomed.2006.12.007},
	File = {:home/agk29/Documents/MendeleyDesktop/1-s2.0-S0891584906008033-main.pdf:pdf},
	Isbn = {0891-5849 (Print)$\backslash$r0891-5849 (Linking)},
	Issn = {08915849},
	Journal = {Free Radical Biology and Medicine},
	Keywords = {Computational model,Mass balance analysis,Reaction kinetics,Theoretical model,computational model,mass balance analysis,reaction kinetics,theoretical model},
	Number = {6},
	Pages = {811--822},
	Pmid = {17320763},
	Title = {{Vascular and perivascular nitric oxide release and transport: Biochemical pathways of neuronal nitric oxide synthase (NOS1) and endothelial nitric oxide synthase (NOS3)}},
	Volume = {42},
	Year = {2007},
