	Issn = {0027-8424},
	Journal = {Proceedings of the National Academy of Sciences of the United States of America},
	Keywords = {Adult,Humans,Magnetic Resonance Imaging,Male,Middle Aged,Migraine with Aura,Migraine with Aura: physiopathology,Vision Disorders,Vision Disorders: physiopathology,Visual Cortex,Visual Cortex: physiopathology},
	Number = {8},
	Pages = {4687--4692},
	Pmid = {11287655},
	Title = {{Mechanisms of Migraine Aura Revealed by Functional MRI in Human Visual Cortex.}},
	Url = {http://www.pnas.org/cgi/content/long/98/8/4687},
