	Journal = {PLoS Computational Biology},
	Keywords = {Adenosine Triphosphate,Adenosine Triphosphate: biosynthesis,Adenylate Kinase,Adenylate Kinase: metabolism,Animals,Biological,Biophysical Phenomena,Biophysics,Cations,Cations: chemistry,Cell Respiration,Computational Biology,Computer Simulation,Electron Transport,Magnesium,Magnesium: metabolism,Mitochondria,Mitochondria: metabolism,Models,Oxidative Phosphorylation,Oxidoreductases,Oxidoreductases: metabolism,Oxygen,Oxygen: metabolism,Phosphates,Phosphates: metabolism,Rats,Substrate Specificity},
