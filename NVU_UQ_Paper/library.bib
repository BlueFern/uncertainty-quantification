	Keywords = {Animals,Astrocytes,Astrocytes: cytology,Astrocytes: physiology,Calcium,Calcium: metabolism,Cell Membrane,Cell Membrane: metabolism,Cells,Cercopithecus aethiops,Cultured,Dose-Response Relationship,Drug,Electric Stimulation,Electric Stimulation: methods,Gene Expression,Gene Expression: physiology,Glial Fibrillary Acidic Protein,Glial Fibrillary Acidic Protein: metabolism,Green Fluorescent Proteins,Green Fluorescent Proteins: metabolism,Immunoelectron,Immunoelectron: methods,Membrane Potentials,Membrane Potentials: drug effects,Membrane Potentials: physiology,Membrane Potentials: radiation effects,Microscopy,Newborn,Occipital Lobe,Occipital Lobe: cytology,Occipital Lobe: metabolism,Occipital Lobe: ultrastructure,Patch-Clamp Techniques,Phorbols,Phorbols: pharmacology,Rats,Reverse Transcriptase Polymerase Chain Reaction,Reverse Transcriptase Polymerase Chain Reaction: m,Ruthenium Red,Ruthenium Red: pharmacology,TRPV Cation Channels,TRPV Cation Channels: genetics,TRPV Cation Channels: metabolism,Transfection,Transfection: methods},
	Month = {sep},
