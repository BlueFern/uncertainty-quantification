@article{Dahlem2005,
	Abstract = {Spreading depression (SD) is a wave-like phenomenon that spreads through the gray matter of central nervous tissue. The aim of this work is to investigate how cellular energy supply and nitric oxide (NO) influence the recovery period after SD wave propagation. We have examined the SD wave in chicken retina by registration of the intrinsic optical signal (IOS). The changes of the IOS were observed via a microscope, transferred to a photomultiplier and amplified. The IOS of the SD wave consists of two phases. The first phase of IOS coexists with cellular swelling induced by ion distribution; the second phase is thought to reflect metabolic changes and reflects the refractory (recovery) period. To analyze the IOS, the amplitude, the duration and the front and the back maximal slopes of the both phases were analyzed. To reduce the cellular level of ATP the blocker of glucose transport-dexamethasone (glucocorticoid hormone) and the blocker of the respiratory chain-potassium cyanide were used. Sodium nitroprusside and trinitroglycerine were chosen as NO-donors. Our results show that during and after SD wave propagation (i) increased NO concentration changes the first and the second phases of IOS (duration of both phases is NO independent), (ii) reduced glucose uptake leads to an increased second phase duration and (iii) block of the respiratory chain prolongs the first phase. According to the results here presented, we propose that glycogen synthesis is one of the mechanisms reflected by the second phase of the IOS.},
	Author = {Dahlem, Yuliya a and Hanke, Wolfgang},
	Doi = {10.1016/j.brainres.2005.04.059},
	Issn = {0006-8993},
	Journal = {Brain research},
	Keywords = {Analysis of Variance,Animals,Chickens,Cortical Spreading Depression,Cortical Spreading Depression: physiology,Energy Metabolism,Energy Metabolism: physiology,Light,Neural Inhibition,Neural Inhibition: physiology,Neurons,Neurons: metabolism,Nitric Oxide,Nitric Oxide Synthase,Nitric Oxide Synthase: metabolism,Nitric Oxide: metabolism,Nonparametric,Radiation,Retina,Retina: cytology,Retina: physiology,Scattering,Spectrum Analysis,Statistics},
	Month = {jul},
	Number = {1},
	Pages = {15--24},
	Pmid = {15935331},
	Title = {{Intrinsic optical signal of retinal spreading depression: second phase depends on energy metabolism and nitric oxide.}},
