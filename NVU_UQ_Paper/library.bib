@article{Gruetter1998a,
	Abstract = {Understanding the mechanism of brain glucose transport across the blood-brain barrier is of importance to understanding brain energy metabolism. The specific kinetics of glucose transport have been generally described using standard Michaelis-Menten kinetics. These models predict that the steady-state glucose concentration approaches an upper limit in the human brain when the plasma glucose level is well above the Michaelis-Menten constant for half-maximal transport, Kt. In experiments where steady-state plasma glucose content was varied from 4 to 30 mM, the brain glucose level was a linear function of plasma glucose concentration. At plasma concentrations nearing 30 mM, the brain glucose level approached 9 mM, which was significantly higher than predicted from the previously reported Kt of approximately 4 mM (p {\textless} 0.05). The high brain glucose concentration measured in the human brain suggests that ablumenal brain glucose may compete with lumenal glucose for transport. We developed a model based on a reversible Michaelis-Menten kinetic formulation of unidirectional transport rates. Fitting this model to brain glucose level as a function of plasma glucose level gave a substantially lower Kt of 0.6 +/- 2.0 mM, which was consistent with the previously reported millimolar Km of GLUT-1 in erythrocyte model systems. Previously reported and reanalyzed quantification provided consistent kinetic parameters. We conclude that cerebral glucose transport is most consistently described when using reversible Michaelis-Menten kinetics.},
	Author = {Gruetter, R and Ugurbil, K and Seaquist, E R},
	Issn = {0022-3042},
	Journal = {Journal of neurochemistry},
	Keywords = {Adult,Biological,Biological Transport,Biological Transport: physiology,Brain,Brain: anatomy {\&} histology,Brain: metabolism,Glucose,Glucose: metabolism,Homeostasis,Homeostasis: physiology,Humans,Kinetics,Magnetic Resonance Imaging,Magnetic Resonance Spectroscopy,Middle Aged,Models,Osmolar Concentration},
	Month = {jan},
	Number = {1},
	Pages = {397--408},
	Pmid = {9422387},
	Title = {{Steady-state cerebral glucose concentrations and transport in the human brain.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/9422387},
	Volume = {70},
	Year = {1998},
	Bdsk-Url-1 = {http://www.ncbi.nlm.nih.gov/pubmed/9422387}}

@article{Brown2013,
	Abstract = {We consider approaches to the numerical difficulties posed by modelling resistive networks with dynamically changing resistances according to a set of coupled ordinary differential equations (ODEs). The prototype problem is autoregulation on a cerebral microvascular network. In this network the amount of perfusion, or tissue blood supply, is determined by the resistance of the vascular network. This resistance can be dynamically altered to regulate the amount of blood flow and hence maintain a balance of chemicals and nutrients in the tissue. The mechanisms responsible for this are primarily local, and are due to various chemical pathways to the vascular smooth muscle cells lining the arterioles and arteries in the vasculature. These cells contract and dilate, which alters the radii and hence resistance of the vessels. Because of the global coupling induced by the resistive network, the resulting stiff system of odes has a dense Jacobian which precludes the direct application of traditional implicit numerical solution methods. We consider a means of remedying this problem by taking a block diagonal Jacobian approximation which allows use of an implicit method but retains the desirable property of explicit solvers of linear solution time scaling with problem size. {\textcopyright} Austral. Mathematical Soc. 2013.},
	Author = {Brown, Richard G},
	File = {:home/agk29/Documents/MendeleyDesktop/Brown - 2012 - Numerical techniques for dynamic resistive networks.pdf:pdf},
	Issn = {14461811 (ISSN)},
	Journal = {ANZIAM Journal},
	Keywords = {Autoregulation,Brain,Ordinary differential equations,Resistive network,Stiff},
	Number = {SUPPL},
	Pages = {171--186},
	Title = {{Numerical techniques for dynamic resistive networks}},
	Url = {http://www.scopus.com/inward/record.url?eid=2-s2.0-84897828264{\&}partnerID=40{\&}md5=9b1cb67d77306aa7120d18624197168b},
	Volume = {54},
