	Title = {{Mathematical modelling of cerebral blood circulation and cerebral autoregulation: towards preventing intracranial hemorrhages in preterm newborns.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4122005{\&}tool=pmcentrez{\&}rendertype=abstract},
	Volume = {2014},
	Year = {2014},
	Bdsk-Url-1 = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4122005%7B%5C&%7Dtool=pmcentrez%7B%5C&%7Drendertype=abstract},
