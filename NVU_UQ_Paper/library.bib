@article{Perry2014,
	Abstract = {The Valsalva maneuver (VM) produces large and abrupt changes in mean arterial pressure (MAP) that challenge cerebral blood flow and oxygenation. We examined the effect of VM intensity on middle cerebral artery blood velocity (MCAv) and cortical oxygenation responses during (phases I-III) and following (phase IV) a VM. Healthy participants (n = 20 mean $\pm$ SD: 27 $\pm$ 7 years) completed 30 and 90{\%} of their maximal VM mouth pressure for 10 s (order randomized) whilst standing. Beat-to-beat MCAv, cerebral oxygenation (NIRS) and MAP across the different phases of the VM are reported as the difference from standing baseline. There were significant interaction (phase (*) intensity) effects for MCAv, total oxygenation index (TOI) and MAP (all P {\textless}0.01). MCAv decreased during phases II and III (P {\textless}0.01), with the greatest decrease during phase III (-5 $\pm$ 8 and -19 $\pm$ 15 cm{\textperiodcentered}s(-1) for 30 and 90{\%} VM, respectively). This pattern was also evident in TOI (phase III: -1 $\pm$ 1 and -5 $\pm$ 4{\%}, both P {\textless}0.05). Phase IV increased MCAv (22 $\pm$ 15 and 34 $\pm$ 23 cm{\textperiodcentered}s(-1)), MAP (15 $\pm$ 14 and 24 $\pm$ 17 mm Hg) and TOI (5 $\pm$ 6 and 7 $\pm$ 5{\%}) relative to baseline (all P {\textless}0.05). Cerebral autoregulation, indexed, as the {\%}MCAv/{\%}MAP ratio, showed a phase effect only (P {\textless}0.001), with the least regulation during phase IV (2.4 $\pm$ 3.0 and 3.2 $\pm$ 2.9). These data illustrate that an intense VM profoundly affects cerebral hemodynamics, with a reactive hyperemia occurring during phase IV following modest ischemia during phases II and III.},
	Author = {Perry, Blake G and Cotter, James D and Mejuto, Gaizka and M{\"{u}}ndel, Toby and Lucas, Samuel J E},
	Doi = {10.3389/fphys.2014.00349},
	Issn = {1664-042X},
