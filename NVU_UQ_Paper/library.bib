@article{MacVicar2015,
	Abstract = {Neuronal activity results in increased blood flow in the brain, a response named functional hyperemia. Astrocytes play an important role in mediating this response. Neurotransmitters released from active neurons evoke Ca2þ increases in astrocytes, leading to the release of vasoactive metabolites of arachidonic acid from astrocyte endfeet onto blood vessels. Synthesis of prostaglandin E2 (PGE2) and epoxyeicosatrienoic acids (EETs) dilate blood vessels, whereas 20-hydroxyeicosatetraenoic acid (20-HETE) constricts vessels. The release of Kþ from astrocyte endfeet may also contribute to vasodilation. Oxygen modulates astrocyte regulation of blood flow. Under normoxic conditions, astrocytic Ca2þ signaling results in vasodilation, whereas under hyperoxic conditions, vasoconstriction is favored. Astrocytes also contribute to the generation of vascular tone. Tonic release of both 20- HETE and ATP from astrocytes constricts vascular smooth muscle cells, generating vessel tone. Under pathological conditions, including Alzheimer's disease and diabetic reti- nopathy, disruption of normal astrocyte physiology can compromise the regulation of blood flow},
	Author = {MacVicar, Brian A and Newman, Eric A},
	Doi = {10.1101/cshpespect.at020388},
	Isbn = {1943-0264 (Electronic) 1943-0264 (Linking)},
	Issn = {19430264},
