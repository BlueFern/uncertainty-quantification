	Isbn = {1573-6873 (Electronic){\$}\backslash{\$}r0929-5313 (Linking)},
	Issn = {09295313},
	Journal = {Journal of Computational Neuroscience},
	Keywords = {Bifurcation,Glia,Instabilities,Potassium dynamics,Seizures},
	Number = {2},
	Pages = {159--170},
	Pmid = {19169801},
	Title = {{The influence of sodium and potassium dynamics on excitability, seizures, and the stability of persistent states: I. Single neuron dynamics}},
	Volume = {26},
