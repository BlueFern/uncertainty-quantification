	Keywords = {Acetylcholine,Acetylcholine: pharmacology,Animals,Arterioles,Arterioles: metabolism,Blood Pressure,Connexins,Connexins: deficiency,Connexins: genetics,Connexins: metabolism,Gap Junctions,Gap Junctions: metabolism,Immunohistochemistry,Inbred C57BL,KATP Channels,KATP Channels: metabolism,Knockout,Male,Membrane Potentials,Mice,Muscle,Pinacidil,Pinacidil: pharmacology,Signal Transduction,Signal Transduction: drug effects,Smooth,Smooth: blood supply,Time Factors,Vasodilation,Vasodilation: drug effects,Vasodilator Agents,Vasodilator Agents: pharmacology},
	Number = {5},
