@article{Rungta2017,
	Abstract = {Optogenetics is increasingly used to map brain activation using techniques that rely on functional hyperaemia, such as opto-fMRI. Here we test whether light stimulation protocols similar to those commonly used in opto-fMRI or to study neurovascular coupling modulate blood flow in mice that do not express light sensitive proteins. Combining two-photon laser scanning microscopy and ultrafast functional ultrasound imaging, we report that in the naive mouse brain, light per se causes a calcium decrease in arteriolar smooth muscle cells, leading to pronounced vasodilation, without excitation of neurons and astrocytes. This photodilation is reversible, reproducible and energy-dependent, appearing at about 0.5 mJ. These results impose careful consideration on the use of photo-activation in studies involving blood flow regulation, as well as in studies requiring prolonged and repetitive stimulations to correct cellular defects in pathological models. They also suggest that light could be used to locally increase blood flow in a controlled fashion.},
	Author = {Rungta, Ravi L and Osmanski, Bruno-F{\'{e}}lix and Boido, Davide and Tanter, Mickael and Charpak, Serge},
