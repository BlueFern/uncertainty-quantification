	Issn = {1078-8956},
	Journal = {Nature Medicine},
	Keywords = {all,as,being as evolutionary distant,connexin,cytoskeleton,epilep,epilepsy,gap junctions,gj,gjs are known as,in the traditional view,inflammation,migrane,multicellular animal species,neurotoxicity,pannexin,provide intercellular communication in,regeneration,spe-,worm and men},
	Number = {4},
	Pages = {600--604},
	Pmid = {22426419},
	Title = {{Activation of neuronal P2X7 receptor--pannexin-1 mediates death of enteric neurons during colitis}},
	Volume = {18},
