@article{Domijan2006a,
	Abstract = {Oscillations in the concentration of free intracellular calcium ([Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater}]){\$}\backslash{\$}nplay an important role in many cell types. Thus, understanding the{\$}\backslash{\$}nmechanisms underlying Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater}oscillations is of significant{\$}\backslash{\$}nscientific import. There are two basic classes of mechanism that{\$}\backslash{\$}ncause these oscillations: (1) positive and negative feedback from{\$}\backslash{\$}ncalcium to the inositol trisphosphate (IP{\textless}sub{\textgreater}3{\textless}/sub{\textgreater}) receptor,{\$}\backslash{\$}nand (2) positive and negative feedback from calcium to IP{\textless}sub{\textgreater}3{\textless}/sub{\textgreater}{\$}\backslash{\$}nmetabolism. These two classes can be distinguished experimentally{\$}\backslash{\$}nby their different responses to pulses of IP{\textless}sub{\textgreater}3{\textless}/sub{\textgreater}. In general{\$}\backslash{\$}nmost cells will have both types of mechanism present simultaneously.{\$}\backslash{\$}nWe show that, when Ca{\textless}sup{\textgreater}2+{\textless}/sup{\textgreater}oscillations are driven by these{\$}\backslash{\$}ntwo mechanisms at the same time, one mechanism is dominant. As the{\$}\backslash{\$}nstrength of each mechanism is varied, the response of the cell exhibits{\$}\backslash{\$}na threshold phenomenon, being governed either by one mechanism or{\$}\backslash{\$}nthe other, with no ambiguity in the response to a pulse of IP{\textless}sub{\textgreater}3{\textless}/sub{\textgreater}.{\$}\backslash{\$}nWe interpret these results, and other responses to IP{\textless}sub{\textgreater}3{\textless}/sub{\textgreater}{\$}\backslash{\$}npulses, in terms of a fast-slow time scale analysis of the calcium{\$}\backslash{\$}ndynamics, where calcium transport across the cell membrane occurs{\$}\backslash{\$}non a slow time scale. [ABSTRACT FROM AUTHOR] Copyright of Journal{\$}\backslash{\$}nof Nonlinear Science is the property of Springer Science {\&} Business{\$}\backslash{\$}nMedia B.V. and its content may not be copied or emailed to multiple{\$}\backslash{\$}nsites or posted to a listserv without the copyright holder's express{\$}\backslash{\$}nwritten permission. However, users may print, download, or email{\$}\backslash{\$}narticles for individual use. This abstract may be abridged. No warranty{\$}\backslash{\$}nis given about the accuracy of the copy. Users should refer to the{\$}\backslash{\$}noriginal published version of the material for the full abstract.{\$}\backslash{\$}n(Copyright applies to all Abstracts.)},
	Author = {Domijan, M and Murray, R and Sneyd, J},
