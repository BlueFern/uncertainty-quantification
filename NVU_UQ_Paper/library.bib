	Keywords = {Awake animal,Hemodynamics,Imaging,Neurovascular coupling},
	Number = {April 2016},
	Pages = {382--398},
	Pmid = {27908788},
	Publisher = {Elsevier},
	Title = {{Time to wake up: Studying neurovascular coupling and brain-wide circuit function in the un-anesthetized animal}},
	Url = {http://dx.doi.org/10.1016/j.neuroimage.2016.11.069},
	Volume = {153},
