	Pages = {641--646},
	Pmid = {17483201},
	Title = {{Neurovascular coupling in the mammalian brain.}},
	Url = {http://www.ncbi.nlm.nih.gov/pubmed/17483201 http://doi.wiley.com/10.1113/expphysiol.2006.036368},
