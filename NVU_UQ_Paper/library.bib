	Keywords = {{\$}\beta{\$}-amyloid,Alzheimer's disease,BACE-1,blood--brain barrier,endothelium},
	Number = {November},
	Pages = {0271678X15606463},
	Pmid = {26661166},
	Title = {{BACE-1 is expressed in the blood--brain barrier endothelium and is upregulated in a murine model of Alzheimer's disease}},
