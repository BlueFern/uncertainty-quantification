	Number = {2},
	Pages = {667--676},
	Pmid = {19939954},
	Title = {{Rapid rise of extracellular pH evoked by neural activity is generated by the plasma membrane calcium ATPase.}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2822688{\&}tool=pmcentrez{\&}rendertype=abstract},
