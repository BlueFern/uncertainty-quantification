	Isbn = {1529-2401 (Electronic){\$}\backslash{\$}r0270-6474 (Linking)},
	Issn = {0270-6474},
	Journal = {Journal of Neuroscience},
	Keywords = {bold-fmri,modeling,monte carlo,two-photon microscopy},
	Number = {8},
	Pages = {3663--3675},
	Pmid = {25716864},
	Title = {{Quantifying the Microvascular Origin of BOLD-fMRI from First Principles with Two-Photon Microscopy and an Oxygen-Sensitive Nanoprobe}},
	Url = {http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=4339366{\&}tool=pmcentrez{\&}rendertype=abstract},
