	Keywords = {Animals,Aorta,Aorta: metabolism,Connexin 43,Connexin 43: deficiency,Connexin 43: metabolism,Connexins,Connexins: deficiency,Connexins: metabolism,Endothelial Cells,Endothelial Cells: metabolism,Endothelium,Inbred C57BL,Knockout,Male,Mice,Tissue Distribution,Vascular,Vascular: metabolism},
	Month = {mar},
	Number = {3},
	Pages = {H1199----H1205},
	Pmid = {16284228},
	Title = {{Connexin40 and connexin43 in mouse aortic endothelium: evidence for coordinated regulation.}},
