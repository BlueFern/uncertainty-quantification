	Journal = {Frontiers in Computational Neuroscience},
	Keywords = {astrocyte modeling,astrocyte-synapse interactions,cortical maps,gliotransmission,metaplasticity,synaptic plasticity},
	Number = {December},
	Pages = {1--25},
	Pmid = {23267326},
	Title = {{Computational quest for understanding the role of astrocyte signaling in synaptic transmission and plasticity}},
