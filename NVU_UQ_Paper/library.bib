@article{Buxton2002,
	Abstract = {The central idea of the oxygen limitation model is that the oxygen extraction fraction E must decrease during neuronal activation, so a large change in cerebral blood flow (CBF) is required to support a small change in the cerebral metabolic rate of O2 (CMRO2). The model is expanded here to show that maintaining mitochondrial pO2 at a constant non-zero level during activation, so that O2 availability does not become limiting for CMRO2, still requires the fractional CBF change to be several times larger than the CMRO2 change. Furthermore, the expanded model also allows for a brief initial increase in E at stimulus onset with a corresponding transient dip in cytoplasm and mitochondrial pO2. However, because the tissue pO2 must increase to support a higher CMRO2, the tissue O2 content itself would be an ambiguous signal for regulating CBF. For this reason, although the function served by a large CBF increase may be to support CMRO2, the tissue O2 content itself is probably not the key factor for regulation of CBF.},
	Author = {Buxton, Richard B},
	Doi = {10.1016/S0531-5131(02)00169-3},
	Issn = {05315131},
	Journal = {International Congress Series},
	Keywords = {1,cbf,cerebral blood flow,cerebral metabolic rate of,cmro 2,increase more than oxygen,introduction,metabolism with,neuronal activation,neurovascular coupling,oxygen,oxygen limitation model,why does blood flow},
	Pages = {23--32},
	Title = {{Coupling between CBF and CMRO2 during neuronal activity}},
