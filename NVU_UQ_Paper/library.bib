	Journal = {Brain Research},
	Keywords = {Cerebral cortex,Hippocampus,Microelectrode,Nitric oxide dynamics,Nitric oxide synthase,Striatum},
	Pages = {1--11},
	Pmid = {24495843},
	Title = {{The pattern of glutamate-induced nitric oxide dynamics in vivo and its correlation with nNOS expression in rat hippocampus, cerebral cortex and striatum}},
